{
  "name": "express",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "npx tsc",
    "start": "node index.js",
    "dev": "NODE_ENV=local nodemon --watch \"src/**/*.ts\" --exec \"ts-node\" ./src/index.ts",
    "test": "jest --runInBand --detectOpenHandles",
    "schema:drop": "ts-node ./src/components/database/database.reset.ts",
    "schema:sync": "ts-node ./src/components/database/database.setup.ts"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cls-hooked": "^4.3.4",
    "@types/hpp": "^0.2.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.4.8",
    "async_hooks": "^1.0.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "cls-hooked": "^4.2.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.1",
    "mysql2": "^3.6.0",
    "reflect-metadata": "^0.1.13",
    "ts-jenum": "^2.2.2",
    "typedi": "^0.10.0",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "testcontainers": "9.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
